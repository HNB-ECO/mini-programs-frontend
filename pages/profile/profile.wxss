page {
    height: 100%;
    position: relative;
}

/* view, text {
    color: #333;
} */

.bg-default {
    padding-top: 0;
}

.profile {
    height: 192rpx;
    /* background: url('https://images.wohoney.com/l4r72kkx8vs'); */
    padding-top: 35rpx;
    box-sizing: border-box;
    background-size: cover;
    background-color: #FDF9F8;
}

.profile .avatar {
    width: 120rpx;
    height: 120rpx;
    border-radius: 100%;
    border: 2px solid white;
    margin-left: 40rpx;
    margin-right: 20rpx;
    vertical-align: top;
}
.profile .btn-login{
    width:160rpx;
    height: 50rpx;
    border: 4rpx solid white;
    background: rgba(255,255,255,0.2);
    color: white;
    text-align: center;
    font-size: 28rpx;
    line-height: 45rpx;
    display:inline-block;
    float:right;
    margin-right:50rpx;
    margin-top:-85rpx;
}
.profile .info {
    display: inline-block;
    vertical-align: middle;
    font-size: 34rpx;
    padding-left: 10rpx;
}
.profile .info .sex-view {
    width: 35rpx;
    height: 35rpx;
}
.profile .info .sex-view image {
    width: 100%;
    height: 100%;
}
.profile .info view {
    font-size: 38rpx;
}

.mywallet-view {
    background: -webkit-linear-gradient(left, #FFAAB0, #FDC8CC);
    width: 300rpx;
    border-radius:82rpx 0 0 82rpx;
    color:white;
    font-size:28rpx;
    float:right;
    padding:10rpx 30rpx;
    margin-top: 20rpx;
}
.mywallet-view .left-icon { 
    width: 56rpx;
    height: 56rpx;
    display: inline-block;
    margin-right: 20rpx;
}
.mywallet-view .left-icon image{
    width: 100%;
    height: 100%;
    float: left;
    margin-top: 5rpx;
}
.mywallet-view .right-item {
    line-height: 130%;
}
.item {
    position: relative;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    padding: 0 20rpx;
    background: white;
}

.item-note {
    font-size: 26rpx;
    color: #A9A9A9;
}

.order-type {
    background: white;
    padding: 0 20rpx;
}

.type-item {
    width: 33.3%;
    display: inline-block;
    text-align: center;
    border-top: 1px solid #F5F5F5;
    border-bottom: 1px solid #F5F5F5;
    padding-top: 40rpx;
    padding-bottom: 20rpx;
    position: relative;
}

.type-item view {
    font-size: 26rpx;
    color: #666;
    line-height: 200%;
    margin-top: 15rpx;
    margin-top: 15rpx;
}

.type-item image {
    width: 50rpx;
    height: 50rpx;
}

.type-item text {
    display: inline-block;
    width: 35rpx;
    height: 35rpx;
    color: #8597FD;
    border-radius: 100%;
    border: 2px solid #8597FD;
    font-size: 26rpx;
    line-height: 35rpx;
    text-align: center;
    position: absolute;
    top: 13%;
    right: 30%;
}

.latest-message {
    background: white;

}
.latest-message text{
    color:#E7E7E7;
    position: absolute;
    right: -5.5px;
    top: 10.5px;
    font-size: 15px;
}

.latest-message .left, .latest-message .right {
    display: inline-block;
    vertical-align: middle;
    padding: 20rpx;
    color: #999999;
    line-height: 180%;
}

.latest-message .left {
    font-size: 10px;
    border-right: 4rpx solid #E7E7E7;
    padding: 10px 2.5px;
    margin: 15rpx;
    padding-right: 12.5px;
    margin-right: 10rpx;
    position: relative;
}

.latest-message .left view {
    color: #999999;
}

.latest-message image {
    width: 72rpx;
    height: 72rpx;
    vertical-align: middle;
    margin-left: 20rpx;
    margin-right: 4rpx;
}

.latest-message .right {
    color: #8597FD;
    font-size: 25rpx;
}

.latest-message .right view {
    font-size: 27rpx;
    color: #999;
    line-height: 180%;
}
.copoun-nav{
    background: white;
    margin-top: 20rpx;
    padding: 25rpx;
    font-size: 28rpx;
    color: #333;
}
.copoun-nav .coupon{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-right: 20rpx;
}
.copoun-nav text{
    position: relative;
    top:2rpx;
    color: #333;
}
.copoun-nav .right{
    width: 22rpx;
    height: 22rpx;
    float: right;
    margin-top: 12rpx;
}
.status{
    color: #8597FD !important;
}
.circle{
    background: white;
}

.customer-nav {
    margin-top: 0;
    border-top: 1px solid #F5F5F5;
    text-align:left;
    line-height:0;
}

.changeid {
    text-align: center;
}
.changeid text {
    color: red;
}