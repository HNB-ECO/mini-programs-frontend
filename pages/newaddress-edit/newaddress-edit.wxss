/* pages/newaddress-add/newaddress-add.wxss */

.bg-default {
    padding-top: 70px;
    background-color: white;
}
.text-input-item {
    margin: 0 30rpx;
    border-bottom: 1px #e8e8e8 solid;
    background-color: white;
    font-size: 34rpx;
    line-height: 160rpx;
}
.text-input-item .text {
    width: 170rpx;
}
.text-input-item .input input {
    margin-right: 20rpx;
    margin-top: 55rpx;
    font-size: 34rpx;
}
.text-input-item .right-icon {
    width: 24rpx;
    height: 24rpx;
}

.btn-view {
    margin-top: 50rpx;
    margin:50rpx 20rpx 0;
}
.btn-view view {
    line-height: 100rpx;
    text-align: center;
    margin: 0 10rpx;
    color: white;
    font-size: 30rpx;
    border-radius: 5rpx;
}
.btn-view .left-btn {
    background-color: #d8644b;
}
.btn-view .right-btn {
    background-color: #4bd863;
}